<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="#D53269"
                android:endColor="#670E2C"/>
            <corners android:radius="5dp"/>
            <stroke android:width="2dp" android:color="#500202"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#F16595"
                android:endColor="#9E073A"/>
            <corners android:radius="5dp"/>
            <stroke android:width="2dp" android:color="#500202"/>
        </shape>
    </item>
</selector>